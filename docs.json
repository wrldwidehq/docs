{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "WRLDWIDE AI",
  "colors": {
    "primary": "#7c5cfc",
    "light": "#a78bfa",
    "dark": "#6a4ce8"
  },
  "favicon": "/favicon.svg",
  "background": {
    "color": {
      "dark": "#0c0c14",
      "light": "#fafafa"
    }
  },
  "appearance": {
    "default": "dark"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "en/introduction",
              "en/getting-started"
            ]
          }
        ]
      },
      {
        "tab": "Platform",
        "groups": [
          {
            "group": "Dark Factory",
            "pages": ["en/features/dark-factory"]
          },
          {
            "group": "Node System",
            "pages": ["en/features/node-system"]
          },
          {
            "group": "Workflows",
            "pages": ["en/features/workflows"]
          }
        ]
      },
      {
        "tab": "Plans & Pricing",
        "groups": [
          {
            "group": "Pricing",
            "pages": ["en/features/plans-overview"]
          }
        ]
      },
      {
        "tab": "Features",
        "source": "en/features"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.wrldwide.ai/dashboard",
          "icon": "gauge-high"
        },
        {
          "anchor": "Platform",
          "href": "https://wrldwide.ai",
          "icon": "rocket"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://wrldwide.ai"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@wrldwide.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.wrldwide.ai/signup"
    }
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/wrldwide_ai",
      "instagram": "https://instagram.com/wrldwide.ai"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "cursor",
      "vscode"
    ]
  }
}
