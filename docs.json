{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "WRLDWIDE AI",
  "colors": {
    "primary": "#7c5cfc",
    "light": "#a78bfa",
    "dark": "#6a4ce8"
  },
  "favicon": "/favicon.svg",
  "background": {
    "color": {
      "dark": "#0c0c14",
      "light": "#fafafa"
    }
  },
  "appearance": {
    "default": "dark"
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Getting Started",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "en/index",
              "en/quickstart"
            ]
          }
        ]
      },
      {
        "tab": "Platform",
        "groups": [
          {
            "group": "Core Platform",
            "pages": [
              "en/dark-factory",
              "en/node-system",
              "en/workflows"
            ]
          }
        ]
      },
      {
        "tab": "Plans & Pricing",
        "groups": [
          {
            "group": "Pricing",
            "pages": [
              "en/plans-overview"
            ]
          }
        ]
      },
      {
        "tab": "AI Tools",
        "groups": [
          {
            "group": "Development",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Dashboard",
          "href": "https://app.wrldwide.ai/dashboard",
          "icon": "gauge-high"
        },
        {
          "anchor": "Platform",
          "href": "https://wrldwide.ai",
          "icon": "rocket"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg",
    "href": "https://wrldwide.ai"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@wrldwide.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get Started",
      "href": "https://app.wrldwide.ai/signup"
    }
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/wrldwide_ai",
      "instagram": "https://instagram.com/wrldwide.ai"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "cursor",
      "vscode"
    ]
  }
}
